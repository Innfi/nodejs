// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:root@localhost:3306/innfi_prisma"
}

model User {
  id Int @default(autoincrement()) @id
  email String @unique
  name String?
}

model Product {
  id Int @default(autoincrement()) @id
  name String 
  code String @unique
  userId Int
}

